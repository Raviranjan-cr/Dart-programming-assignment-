void main() {
  Map<String, int> map = {'a': 1, 'b': 2, 'c': 3};

  // Add a key-value pair
  map['d'] = 4;
  print(map);

  // Update a value
  map['b'] = 5;
  print(map);

  // Remove a key-value pair
  map.remove('c');
  print(map);
}

output 

{a: 1, b: 2, c: 3, d: 4}
{a: 1, b: 5, c: 3, d: 4}
{a: 1, b: 5, d: 4}
